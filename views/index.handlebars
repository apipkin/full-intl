<dl class="main">
    <dt>Message</dt>
    <dd><dl>
        <dt>&lcub;&lcub;intlMessage MESSAGE FIRSTNAME=FIRSTNAME LASTNAME=LASTNAME NUM_BOOKS=NUM_BOOKS}}</dt>
        <dd>
            {{intlMessage MESSAGE FIRSTNAME=FIRSTNAME LASTNAME=LASTNAME NUM_BOOKS=NUM_BOOKS}}
        </dd>
    </dl></dd>

    <dt>Number</dt>
    <dd><dl>
        <dt>&lcub;&lcub;intl NUM_BOOKS}}</dt>
        <dd>{{intl NUM_BOOKS}}</dd>

        <dt>&lcub;&lcub;intlNumber LONG_NUM}}</dt>
        <dd>{{intlNumber LONG_NUM}}</dd>

        <dt>&lcub;&lcub;intl NUM_BOOKS style="percent"}}</dt>
        <dd>{{intl NUM_BOOKS style="percent"}}</dd>

        <dt>&lcub;&lcub;intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2}}</dt>
        <dd>{{intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2}}</dd>


        <dt>&lcub;&lcub;intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2 locale="en-AU"}}</dt>
        <dd>{{intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2 locale="en-AU"}}</dd>
    </dl></dd>

    <dt>Date</dt>
    <dd><dl>

        <dt>&lcub;&lcub;intl NOW_DATE}}</dt>
        <dd>{{intl NOW_DATE}}</dd>

        <dt>&lcub;&lcub;intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric"}}</dt>
        <dd>{{intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric"}}</dd>

        <dt>&lcub;&lcub;intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric" timeZone="UTC" timeZoneName="short"}}</dt>
        <dd>{{intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric" timeZone="UTC" timeZoneName="short"}}</dd>

        <dt>&lcub;&lcub;intl NOW_DATE hour="numeric" minute="numeric" second="numeric" timeZoneName="short" }}</dt>
        <dd>{{intl NOW_DATE hour="numeric" minute="numeric" second="numeric" timeZoneName="short" }}</dd>

    </dl></dd>

    {{#intl locale="ru-RU"}}

    <dt>&lcub;&lcub;#intl locale="ru-RU"}}</dt>
    <dd><dl class="main">

        <dt>Message</dt>
        <dd><dl>
            <dt>&lcub;&lcub;intlMessage MESSAGE FIRSTNAME=FIRSTNAME LASTNAME=LASTNAME NUM_BOOKS=NUM_BOOKS}}</dt>
            <dd>
                {{intlMessage MESSAGE FIRSTNAME=FIRSTNAME LASTNAME=LASTNAME NUM_BOOKS=NUM_BOOKS}}
            </dd>
        </dl></dd>

        <dt>Number</dt>
        <dd><dl>
            <dt>&lcub;&lcub;intl NUM_BOOKS}}</dt>
            <dd>{{intl NUM_BOOKS}}</dd>

            <dt>&lcub;&lcub;intlNumber LONG_NUM}}</dt>
            <dd>{{intlNumber LONG_NUM}}</dd>

            <dt>&lcub;&lcub;intl NUM_BOOKS style="percent"}}</dt>
            <dd>{{intl NUM_BOOKS style="percent"}}</dd>

            <dt>&lcub;&lcub;intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2}}</dt>
            <dd>{{intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2}}</dd>


            <dt>&lcub;&lcub;intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2 locale="en-AU"}}</dt>
            <dd>{{intl NUM_BOOKS currency="USD" style="currency" minimumFractionDigits=2 maximumFractionDigits=2 locale="en-AU"}}</dd>
        </dl></dd>

        <dt>Date</dt>
        <dd><dl>

            <dt>&lcub;&lcub;intl NOW_DATE}}</dt>
            <dd>{{intl NOW_DATE}}</dd>

            <dt>&lcub;&lcub;intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric"}}</dt>
            <dd>{{intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric"}}</dd>

            <dt>&lcub;&lcub;intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric" timeZone="UTC" timeZoneName="short"}}</dt>
            <dd>{{intl NOW_DATE weekday="long" year="numeric" month="long" day="numeric" timeZone="UTC" timeZoneName="short"}}</dd>

            <dt>&lcub;&lcub;intl NOW_DATE hour="numeric" minute="numeric" second="numeric" timeZoneName="short" }}</dt>
            <dd>{{intl NOW_DATE hour="numeric" minute="numeric" second="numeric" timeZoneName="short" }}</dd>

        </dl></dd>


    </dd>
    <dt>&lcub;&lcub;/intl}}</dt>
</dl>
    {{/intl}}

</dl>


